{
	"coregistration_wf": {
		"t1_to_source": {
			"cost": "mutualinfo",
			"dof": 6
		},
		"source_to_t1": {
			"initial_moving_transform_com": 1,
			"dimension": 3,
			"transforms": [ "Rigid" ],
			"transform_parameters": [ [ 0.2 ] ],
			"number_of_iterations": [ [ 500, 250, 50 ] ],
			"metric": [ "MI" ],
			"metric_weight": [ 1.0 ],
